#! /bin/python
def example_plot:
     from pylab import plot,show
     x = [ 0.5, 1.0, 2.0, 4.0, 7.0, 10.0 ]
     y = [ 1.0, 2.4, 1.7, 0.3, 0.6, 1.8 ]
     plot(x,y)
     show()

def sin_plot:
     from pylab import plot,show
     from numpy import linspace,sin
     x = linspace(0,10,100)
     y = sin(x)
     plot(x,y)
     show()

